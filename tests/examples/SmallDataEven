	var autoOpenchat = true;

  (function() {
    var se = document.createElement('script'); se.type = 'text/javascript'; se.async = true;
    se.src = '//commondatastorage.googleapis.com/code.snapengage.com/js/99a60646-08f1-4b62-8106-7e71ee53454b.js';
    var done = false;
    se.onload = se.onreadystatechange = function() {
      if (!done&&(!this.readyState||this.readyState==='loaded'||this.readyState==='complete')) {
        done = true;
        /* Place your SnapEngage JS API code below */
        /* SnapEngage.allowChatSound(true); Example JS API: Enable sounds for Visitors. */
        SnapEngage.hideButton();
        SnapEngage.allowChatSound(false);

        SnapEngage.setCallback('Close', function (type, status) {
        	window.autoOpenchat = false; // Que no se vuelva a abrir solo
        	document.title = app.title;
        	SnapEngage.allowChatSound(false);
		    SnapEngage.hideButton();
		});

		SnapEngage.setCallback('OpenProactive', function (agent, msg) {
			if(window.autoOpenchat) {
				document.title = app.title;
				SnapEngage.closeForm()
		    } else {
		    	SnapEngage.allowChatSound(true);
		    	_gaq.push(['_trackEvent', 'SnapEngage', 'proactivePrompt', agent]);
		    }
		});
      }
    };
